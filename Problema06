main
    add v2,zr,0x0040
    add v3,zr,0x0061
    add v4,zr,1
fill
    stb v4,v2
    add v2,v2,1
    add v4,v4,1
    bne v2,v3,fill

    add a0,zr,0x0040
    add a1,zr,0x0060
loop
    bge a0,a1,end
    lbu v0,a0
    lbu v1,a1
    stb v1,a0
    stb v0,a1
    add a0,a0,1
    sub a1,a1,1
    beq zr,zr,loop

end
    hlt
