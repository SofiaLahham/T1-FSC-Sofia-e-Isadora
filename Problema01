main
    ldw v0,zr,n      ; v0 = N
    add v1,zr,0x0020 ; v1 = endereço inicial (0x20)
    add v2,zr,1      ; v2 = primeiro ímpar

loop
    stw v2,v1        ; grava ímpar na memória
    add v2,v2,2      ; próximo ímpar
    add v1,v1,2      ; próximo endereço
    sub v0,v0,1      ; N--
    bne v0,zr,loop   ; repete se N > 0

end
    hlt              ; fim

n
    7               
